# platform :ios, '9.0'

target 'RedEye' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RedEye
  pod ‘Firebase/Core’
  pod ‘Firebase/Auth’
  pod ‘Firebase/Database’
  pod ‘Firebase/Storage’
  pod 'SwiftKeychainWrapper'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod ‘Alamofire’
  pod 'SDWebImage', '~>3.8'
end
